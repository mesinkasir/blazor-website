@page "/fitur"
@inject HttpClient Http
@if (contents == null){<p><em>Loading...</em></p>}else {@foreach (var content in contents){
<PageTitle>@content.Fitur</PageTitle>
<HeadContent>
<meta name="description" content="@content.Fiturdeskripsi">
<meta content='@content.Fitur' property='og:title'/>
<meta content='https://www.fiverr.com/creativitas/create-your-custom-website-and-app' property='og:author'/>
<meta content='@content.Fitur' name='facebook:title'/>
<meta content='website' property='og:type'/>
<meta content='https://www.facebook.com/mesinkasircomplete' property='facebook:author'/>
<meta content='@content.Cover' name='facebook:image'/>
<meta content='@content.Fitur' property='og:site_name'/>
<meta content='@content.Fiturdeskripsi' property='facebook:description'/>
<meta content='summary_large_image' name='twitter:card'/>
<meta content='https://twitter.com/hockeycomp' property='twitter:author'/>
<meta content='@content.Cover' name='twitter:image'/>
<meta content='@content.Fiturdeskripsi' name='twitter:description'/>
<meta content='@content.Fitur' name='twitter:title'/>
<meta name="robots" content="index, follow">
</HeadContent>
<h1><strong><a href="/">@content.Fitur</a></strong></h1>
<h2>@content.Fiturdeskripsi</h2>
<div class="ratio ratio-16x9">
  <iframe src="https://www.youtube.com/embed/@content.Fiturembed" title="@content.Fitur" allowfullscreen></iframe>
</div>
<p>@content.Fiturtext</p>
}}
@code {
    private FiturContent[]? contents;
    protected override async Task OnInitializedAsync()
    {
        contents = await Http.GetFromJsonAsync<FiturContent[]>("data/page.json");
    }
    public class FiturContent
    {
        public string? Fitur { get; set; }
        public string? Fiturembed { get; set; }
        public string? Fiturdeskripsi { get; set; }
        public string? Fiturtext { get; set; }
        public string? Cover { get; set; }
    }
}